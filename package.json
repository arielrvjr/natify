{
  "name": "nativefy-framework",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' + && rm -f pnpm-lock.yaml",
    "clean:install": "pnpm clean && pnpm install",
    "android": "turbo run android --filter=examples",
    "ios": "pnpm --filter examples ios",
    "registry:start": "docker-compose -f verdaccio/docker-compose.yml up -d",
    "registry:stop": "docker-compose -f verdaccio/docker-compose.yml down",
    "registry:logs": "docker-compose -f verdaccio/docker-compose.yml logs -f",
    "registry:publish": "pnpm build && pnpm -r --filter '@nativefy/*' publish --registry http://localhost:4873 --no-git-checks",
    "registry:publish:all": "pnpm build && pnpm -r --filter '@nativefy/*' publish --registry http://localhost:4873 --no-git-checks --force"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "overrides": {
      "react": "19.1.1"
    }
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.83.0",
    "@react-native/typescript-config": "0.83.0",
    "@types/react": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.0.3",
    "react": "^19.1.1",
    "react-native": "^0.82.1",
    "turbo": "^2.6.2",
    "typescript": "^5.9.3"
  }
}
